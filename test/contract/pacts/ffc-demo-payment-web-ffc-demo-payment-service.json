{
  "consumer": {
    "name": "ffc-demo-payment-web"
  },
  "provider": {
    "name": "ffc-demo-payment-service"
  },
  "interactions": [
    {
      "description": "get all schedules",
      "providerState": "schedules exist",
      "request": {
        "method": "GET",
        "path": "/schedule",
        "headers": {
          "Authorization": "Bearer abc-123"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "claimId": "MINE123",
            "paymentDate": "2015-08-06T16:53:10+01:00"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].claimId": {
            "match": "type"
          },
          "$.body[*].paymentDate": {
            "match": "iso8601DateTimeWithMillis"
          }
        }
      }
    },
    {
      "description": "get schedule by claim id",
      "providerState": "schedule exists",
      "request": {
        "method": "GET",
        "path": "/schedule/MINE001"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "claimId": "MINE001",
            "paymentDate": "2015-08-06T16:53:10+01:00"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].claimId": {
            "match": "type"
          },
          "$.body[*].paymentDate": {
            "match": "iso8601DateTimeWithMillis"
          }
        }
      }
    },
    {
      "description": "schedule by claim id",
      "providerState": "schedule doesn't exist",
      "request": {
        "method": "GET",
        "path": "/schedule/NOTMINEWONTFIND"
      },
      "response": {
        "status": 404,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}
